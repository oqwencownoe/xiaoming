//糖果平均寿命
#include <iostream>
#include <cstdio>
using namespace std;
int a[12];
int b[12];
int c[12]={31,28,31,30,31,30,31,31,30,31,30,31};
int main(){
  for(int i=1;i<=12;i++){
    cin>>a[i];
  }
  for(int i=1;i<=12;i++){
    int sum=0;
    for(int j=0;j<=i-1;j++){
      sum+=c[j];
    }
    b[i]=sum;
  }
  for(int i=1;i<=12;i++){
    b[i]=a[i]*b[i];
  }

  double sum1=0,sum2=0;
  for(int i=1;i<=12;i++){
    sum1+=a[i];
  }
  for(int i=1;i<=12;i++){
    sum2+=b[i];
  }

  double ans=sum2/sum1;
  cout<<ans<<endl;
  return 0;
}
